<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:id="@+id/drawerLayout"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.shivang.codex.FuncActivity">
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="6"
    android:id="@+id/funcLinLayout">
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/NavDrarButton"
            android:onClick="onNavDrarButton"
            tools:background="?android:attr/floatingToolbarOpenDrawable" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/meet4"
        android:layout_marginTop="@dimen/_15sdp"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/newFuncB1"
        android:text="Book A Meet"
        android:onClick="onMeetBook"
        />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:src="@drawable/mylocation2"
            android:layout_marginTop="@dimen/_15sdp"
            android:id="@+id/imageView3" />

        <Button
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:text="My Location"
        android:onClick="onMyLoc"
        android:id="@+id/newFuncB2"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/contacts"
        android:layout_marginTop="@dimen/_15sdp"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:text="Contacts"
        android:id="@+id/newFuncB3"
        android:onClick="onContactImp"
        android:layout_marginBottom="@dimen/_15sdp"/>
</LinearLayout>
    <android.support.design.widget.NavigationView
        android:id="@+id/navigationView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:menu="@menu/check"
        android:background="#81C784"
        app:itemIconTint="@android:color/transparent"
        android:layout_gravity="start"
        >

    </android.support.design.widget.NavigationView>
    </android.support.v4.widget.DrawerLayout>